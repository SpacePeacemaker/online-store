<div class="Header-slider">
    <div class="Slider Slider_main">
      <div class="Slider-box">
        {% cache cache_time banners_objects %}
            {% for banner in banners %}
                <div class="Slider-item">
                  <div class="Slider-content">
                    <div class="row">
                      <div class="row-block">
                        <strong class="Slider-title">{{ banner.product.name }}<span class="text_primary"> example</span> example
                        </strong>
                        <div class="Slider-text">Example text
                        </div>
                        <div class="Slider-footer"><a class="btn btn_primary" href="#">Lets go</a>
                        </div>
                      </div>
                      <div class="row-block">
                        <div class="Slider-img"><img src="{{ banner.image.url }}" alt="banner"/>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
            {% endfor %}
        {% endcache %}
      </div>
      <div class="Slider-navigateWrap">
        <div class="Slider-navigate">
        </div>
      </div>
    </div>
</div>
